	"bugs": {
		"url": "https://github.com/adraffy/cid.js/issues"
	},
	"homepage": "https://github.com/adraffy/cid.js#readme",
	"author": {
		"name": "raffy.eth",
		"email": "raffy@me.com",
		"url": "http://raffy.antistupid.com"
	},
	"type": "module",
	"scripts": {
		"test": "node test/Multibase.js && node test/RFC4648.js && node test/Base58BTC.js",
		"build": "rollup -c"
	},
	"devDependencies": {
		"@rollup/plugin-terser": "^0.4.4",
		"rollup": "^4.9.4"
	}
