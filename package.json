{
	"name": "@adraffy/cid",
	"version": "0.0.1",
	"description": "Multiformat CID",
	"keywords": [
		"CID",
		"Multiformat",
		"Multihash",
		"Multibase",
		"uvarint",
		"Base64",
		"Base58",
		"Base32"
	],
	"exports": {
